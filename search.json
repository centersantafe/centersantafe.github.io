---
[

  {% assign docs = site.pages | where_exp: "page", "page.url contains 'artists'" %}
  {% for doc in docs %}
    {
    "title": {{ doc.title | jsonify }} ,
    "url": {{ doc.url | relative_url | jsonify }},
    "content": {{ doc.content | strip_html | normalize_whitespace | jsonify }}
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %},




  {% assign docs = site.pages | where_exp: "page", "page.url contains '/listings/'" %}
  {% for doc in docs %}
    {
    "title": {{ doc.artist | jsonify }} ,
    "url": {{ doc.url | relative_url | jsonify }},
    "content": {{ doc.content | strip_html | normalize_whitespace | jsonify }}
    }
  {% unless forloop.last %},{% endunless %}
  {% endfor %}
]